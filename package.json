{
  "name": "objio-layout",
  "version": "0.0.13",
  "description": "",
  "main": "index.js",
  "repository": "git@github.com:xegax/objio-layout.git",
  "scripts": {
    "prepare": "npm run clean && tsc -p tsconfig.publish.json -outDir . && npm run copy",
    "postpublish": "npm run clean",
    "copy": "shx cp src/view/**/*.scss view", 
    "dist": "tsc",
    "dev": "npm run prepare",
    "clean": "npm run clean-src && npm run clean-dirs",
    "clean-src": "shx rm -rf src/**/*.js src/**/*.js.map src/**/*.d.ts *.d.ts *.js.map *.js",
    "clean-dirs": "shx rm -rf client server view"
  },
  "author": "xega",
  "license": "ISC",
  "devDependencies": {
  },
  "dependencies": {
    "objio": "^0.0.32",
    "objio-object": "^0.0.12"
  }
}
